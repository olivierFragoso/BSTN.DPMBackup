                <ServiceImplementation
                 description=""
                 handlerName="Script"
                 name="ProcessGABEMaterialMaster">
                    <ConfigurationTables>
                        <ConfigurationTable
                         dataShapeName=""
                         description=""
                         isMultiRow="false"
                         name="Script"
                         ordinal="0">
                            <DataShape>
                                <FieldDefinitions>
                                    <FieldDefinition
                                     baseType="STRING"
                                     description="code"
                                     name="code"
                                     ordinal="0"></FieldDefinition>
                                    <FieldDefinition
                                     baseType="JSON"
                                     description=""
                                     name="editorSettings"
                                     ordinal="0"></FieldDefinition>
                                </FieldDefinitions>
                            </DataShape>
                            <Rows>
                                <Row>
                                    <code>
                                    <![CDATA[
                                    let newMaterialDetails = me.GetMaterialMasterData();
                                    
                                    newMaterialDetails.rows.toArray().forEach(row => {
                                    
                                    //result: integer -- Returns uid if material exists, returns 0 if the material is new
                                    let existingRow = me.CheckForExistingMaterial({
                                    	MaterialID: row.MaterialID /* STRING */
                                    });
                                    
                                    switch (existingRow>0){
                                    	
                                    	case true:
                                    		Things["BSTN.GABEInterface.Manager"].UpdateMaterialMaster({
                                    		uid: existingRow /* INTEGER */,
                                    		siteuid: 1 /* INTEGER */,
                                    		defaultunit: 6 /* INTEGER */,
                                    		enable: 1 /* INTEGER */,
                                    		assemblytype: null /* INTEGER */,
                                    		assemblyrelationship: null /* INTEGER */,
                                    		description: row.Description /* STRING */,
                                    		id: row.MaterialID /* STRING */,
                                    		source: null /* STRING */,
                                    		version: row.Version /* INTEGER */
                                    		});
                                    		
                                    		Things["BSTN.GABEInterface.Manager"].UpdateMaterialMasterProcessedFlag({
                                    			ID: row.ID /* INTEGER [Required] */
                                    		});
                                    
                                    		result = "done";
                                    	break;
                                    
                                    
                                    case false:
                                    		Things["BSTN.GABEInterface.Manager"].InsertNewMaterialMaster({
                                    		siteuid: 1 /* INTEGER */,
                                    		defaultunit: 6 /* INTEGER */,
                                    		enable: 1 /* INTEGER */,
                                    		assemblytype: null /* INTEGER */,
                                    		assemblyrelationship: null /* INTEGER */,
                                    		description: row.Description /* STRING */,
                                    		id: row.MaterialID /* STRING */,
                                    		source: null /* STRING */,
                                    		version: row.Version /* INTEGER */,
                                    		});
                                    
                                    		Things["BSTN.GABEInterface.Manager"].UpdateMaterialMasterProcessedFlag({
                                            ID: row.ID /* INTEGER [Required] */,
                                            });
                                    		result = "done";
                                    	break;
                                    
                                    
                                    default: result = "unknown error";
                                    	break;
                                    }
                                    });
                                    ]]>
                                    </code>
