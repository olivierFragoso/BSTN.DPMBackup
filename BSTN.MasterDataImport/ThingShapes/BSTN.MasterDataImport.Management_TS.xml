                <ServiceImplementation
                 description=""
                 handlerName="Script"
                 name="ProcessGABEWorkMaster">
                    <ConfigurationTables>
                        <ConfigurationTable
                         dataShapeName=""
                         description=""
                         isMultiRow="false"
                         name="Script"
                         ordinal="0">
                            <DataShape>
                                <FieldDefinitions>
                                    <FieldDefinition
                                     baseType="STRING"
                                     description="code"
                                     name="code"
                                     ordinal="0"></FieldDefinition>
                                    <FieldDefinition
                                     baseType="JSON"
                                     description=""
                                     name="editorSettings"
                                     ordinal="0"></FieldDefinition>
                                </FieldDefinitions>
                            </DataShape>
                            <Rows>
                                <Row>
                                    <code>
                                    <![CDATA[
                                    let newWorkMasterDetails = Things["BSTN.GABEInterface.Manager"].GetWorkMasterData();
                                    
                                    
                                    newWorkMasterDetails.rows.toArray().forEach((row) => {
                                      //result: integer -- Returns uid if material exists, returns 0 if the material is new
                                      let existingRow = Things["BSTN.GABEInterface.Manager"].CheckForExistingWorkMaster({
                                        MaterialID: row.MaterialID /* STRING */,
                                        WorkUnit: row.WorkUnit,
                                      });
                                    
                                      switch (existingRow > 0) {
                                        case true:
                                          Things["BSTN.GABEInterface.Manager"].UpdateWorkMaster({
                                            siteuid: 1 /* INTEGER */,
                                            MaterialID: row.MaterialID /* STRING */,
                                            IdealCycleTime: row.IdealCycleTime /* NUMBER */,
                                            WorkUnit: row.WorkUnit /* STRING */,
                                          });
                                    
                                          Things["BSTN.GABEInterface.Manager"].UpdateWorkMasterProcessedFlag({
                                            ID: row.ID /* Integer [Required] */
                                          });
                                    
                                          result = "done";
                                          break;
                                    
                                        case false:
                                          Things["BSTN.GABEInterface.Manager"].InsertNewWorkMaster({
                                            siteuid: 1 /* INTEGER */,
                                            MaterialID: row.MaterialID /* STRING */,
                                            IdealCycleTime: row.IdealCycleTime /* NUMBER */,
                                            WorkUnit: row.WorkUnit /* STRING */,
                                          });
                                    
                                          Things["BSTN.GABEInterface.Manager"].UpdateWorkMasterProcessedFlag({
                                            ID: row.ID /* INTEGER [Required] */,
                                          });
                                          result = "done";
                                          break;
                                    
                                        default:
                                          result = "unknown error";
                                          break;
                                      }
                                    });
                                    ]]>
                                    </code>
