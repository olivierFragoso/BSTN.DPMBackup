                                </DataShape>
                                <Rows>
                                    <Row>
                                        <code>
                                        <![CDATA[
                                        logger.info("Entering {}:DeployComponent::Entry", me.name);
                                        let projectName = me.GetProjectName();
                                        if (!projectName) {
                                        	throw "Project name is missing";
                                        }
                                        let buildingBlockName = projectName.split(".")[1];
                                        if (!buildingBlockName) {
                                        	throw "Building block name is missing";
                                        }
                                        
                                        let manager = projectName + ".Manager";
                                        let baseManager = me.GetConfiguredComponentManager({
                                        	identifier: "ComponentManager"
                                        });
                                        
                                        Things[baseManager].AddGlobalManagerConfigurationEntry({
                                        	managerName: manager,
                                        	key: manager
                                        });
                                        
                                        let result = true;
                                        ]]>
                                        </code>
